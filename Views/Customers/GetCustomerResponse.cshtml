@model IEnumerable<Models.EntityModels.CustomerViewModel>

@{
    ViewData["Title"] = "GetCustomerResponse";
}

<h1>Get Customer Response</h1>
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
            
                <h4 class="header-title mt-0 mb-3">Customers Response</h4>
                <div class="table-responsive dash-social">
                    <table id="datatable" class="table">
                        <thead class="thead-light">
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Query)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.FullName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Phone)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Answer)
                                </th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Query)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FullName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Phone)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Answer)
                                    </td>

                                </tr>
                            }
                        </tbody>
                    </table>
                    </div>
</div>
</div>
</div>
